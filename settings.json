{
  "terminal.integrated.fontSize": 14,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.labelFormat": "short",
  "workbench.editor.wrapTabs": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.restoreViewState": true,
  "workbench.editor.splitOnDragAndDrop": true,
  "workbench.list.smoothScrolling": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorCustomizations": {
    "tab.activeModifiedBorder": "#FDDA0D",
    "tab.activeBorder": "#32CD32",
    "tab.activeBorderTop": "#023020",
  },
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.lineHeight": 1.8,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.renderLineHighlight": "gutter",
  "editor.tokenColorCustomizations": {
    "comments": "#A8A8A880"
  },
  "editor.linkedEditing": true,
  "editor.rulers": [
    80,
    120
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "never"
  },
  "editor.parameterHints.enabled": false,
  "editor.wordWrapColumn": 160,
  "editor.wordWrap": "bounded",
  "editor.acceptSuggestionOnEnter": "on",
  "editor.quickSuggestionsDelay": 150,
  "editor.snippetSuggestions": "bottom",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.selectionHighlight": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.formatOnSave": false,
  "css.completion.completePropertyWithSemicolon": true,
  "css.lint.duplicateProperties": "warning",
  "css.lint.float": "warning",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
  },
  "files.eol": "\n",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": ".eslint*, eslint*, prettier*, tsconfig*, vite*, package-lock*, yarn*, pnpm-lock*",
    ".env.local": ".env*",
    ".env": ".env*"
  },
  "breadcrumbs.enabled": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
  ],
  "html.format.wrapAttributes": "aligned-multiple",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "merge-conflict.diffViewPosition": "Beside",
  "npm-intellisense.importES6": true,
  "npm-intellisense.scanDevDependencies": true,
  "material-icon-theme.files.associations": {
    "ormconfig.json": "database",
    "*.json": "tune",
    "vitest.config.e2e.ts": "vite",
  },
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe",
    "store": "Redux-store",
    "actions": "redux-actions",
    "reducers": "redux-reducer",
    "interfaces": "Rules",
    "use-cases": "project",
    "mappers": "meta",
    "view-models": "views",
    "kafka": "pipe",
    "factories": "class",
    "presentation": "template",
    "workflows": "pipe",
    "domain": "class",
    "value-objects": "decorators",
    "application": "components",
    "enterprise": "environment",
    "subscribers": "messages",
    "units": "ci",
    "presenters": "views",
    "cryptography": "secure",
    "storage": "shared",
    "redis": "database",
    "deploy": "environment"
  },
  "material-icon-theme.activeIconPack": "nest",
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
  "javascript.format.semicolons": "insert",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "typescript.tsserver.log": "verbose",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "json.maxItemsComputed": 99999999,
  "window.newWindowDimensions": "maximized",
  "terminal.integrated.enablePersistentSessions": false,
  "editor.foldingMaximumRegions": 65000,
  "git.mergeEditor": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.formatOnSave": true
  },
  "explorer.confirmDelete": false,
  "cSpell.userWords": [
    "cnpj",
    "datetime",
    "encrypter",
    "fastify",
    "FINCREDIT",
    "fipe",
    "Hasher",
    "pulumi",
    "WITHSCORES",
    "zincrby",
    "zrange"
  ],
  "editor.accessibilitySupport": "off",
  "git.openRepositoryInParentFolders": "never",
  "window.zoomLevel": -1,
  "window.commandCenter": false,
  "terminal.integrated.env.windows": {},
  "editor.stickyScroll.enabled": false,
  "accessibility.signals.lineHasError": {
    "sound": "off"
  },
  "accessibility.signals.lineHasFoldedArea": {
    "sound": "off"
  },
  "accessibility.signals.lineHasBreakpoint": {
    "sound": "off"
  },
  "accessibility.signals.lineHasInlineSuggestion": {
    "sound": "off"
  },
  "accessibility.signals.terminalQuickFix": {
    "sound": "off"
  },
  "accessibility.signals.onDebugBreak": {
    "sound": "off"
  },
  "accessibility.signals.noInlayHints": {
    "sound": "off"
  },
  "accessibility.signals.taskCompleted": {
    "sound": "off"
  },
  "accessibility.signals.taskFailed": {
    "sound": "off"
  },
  "accessibility.signals.terminalCommandFailed": {
    "sound": "off"
  },
  "accessibility.signals.notebookCellCompleted": {
    "sound": "off"
  },
  "accessibility.signals.notebookCellFailed": {
    "sound": "off"
  },
  "accessibility.signals.diffLineInserted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineDeleted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineModified": {
    "sound": "off"
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.defaultFormatter": "kennylong.kubernetes-yaml-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never",
      "source.fixAll": "never"
    },
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.detectIndentation": true,
    "editor.indentSize": "tabSize"
  },
  "prisma.showPrismaDataPlatformNotification": false,
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.hover.delay": 300,
  "workbench.layoutControl.enabled": false,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "security.allowedUNCHosts": [
    "wsl.localhost"
  ],
  "remote.autoForwardPortsSource": "hybrid",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}
